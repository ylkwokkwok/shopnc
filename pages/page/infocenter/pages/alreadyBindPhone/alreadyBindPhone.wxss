.container{
  background: #f6f6f6;
}
.alreadybindphone{
  width: 750rpx;
  background: white;
  padding:20rpx 0;
}
.alreadybindphone .same{
  width: 100%;
  border-bottom:1px solid #ccc;
  height: 100rpx;
  line-height: 100rpx;
  padding:0 20rpx;
}
.alreadybindphone .same label{
  width: 120rpx;
  font-size: 30rpx;
  float: left;
  color: #333;
}
.alreadybindphone .same text{
  float: left;
  margin-left: 10rpx;
  font-size: 30rpx;
  color: #999999;
  height: 100%;
  line-height: 100rpx;
}
.alreadybindphone navigator{
  width: 710rpx;
  margin:20rpx auto;
  height: 90rpx;
  line-height: 90rpx;
  color: white;
  background: #568fdc;
  text-align: center;
  font-size: 34rpx;
  border-radius: 10rpx;
}
.returnInfo{
  position: absolute;
  bottom: 50rpx;
  right: 50rpx;
  font-size: 24rpx;
  text-decoration: underline;
  color: #568fdc;
}