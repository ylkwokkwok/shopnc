page{
  background: #f6f6f6;
}
.form{
  width: 100%;
}
.form .same{
  width: 100%;
  background: white;
  padding:20rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
}
.form .same label,.form .pricegroup .same_one>label{
  width: 120rpx;
  font-size: 28rpx;
  color: #5d5d5d;
  float:left;
  padding-top: 10rpx;
}
.form .same label.black{
  color: #333;
}
.form .same .right{
  width: 200rpx;
  float: right;
  font-size: 28rpx;
  color:#999;
  text-align: right;
}
.form .same .right image,.form .same_one .right image{
  width: 15rpx;
  margin-left: 20rpx;
  float: right;
  margin-top: 8rpx;
}
.form .same .right input{
  float: left;
  width: 320rpx;
  padding-right: 20rpx;
}
.form .same>view{
  padding-top: 10rpx;
}
.form .same>view,.form .same>input{
  float: left;
  font-size: 28rpx;
  padding-left: 30rpx;
}
.form .prompt{
  padding:20rpx 20rpx;
  font-size: 20rpx;
  color: #bbb;
  display: block;
}
.form .same>text{
  display: block;
  color: #5d5d5d;
  font-size: 28rpx;
}
.form textarea{
  width: 710rpx;
  margin-top: 10rpx;
  border-radius: 20rpx;
  background: #eeeeee;
  font-size: 28rpx;
  padding-top: 10rpx;
  padding-left: 20rpx;
  color: #333;
}
.form .pricegroup{
  background: white;
  margin-bottom: 20rpx;
}
.form .pricegroup>text{
  font-size: 28rpx;
  color: #333;
  padding:20rpx;
  display: block;
}
.form .pricegroup .same_one{
  border-top: 1px solid #ccc;
  padding:20rpx;
  overflow: hidden;
}
.form .pricegroup .same_one .right{
  float: right;
  width: 590rpx;
  text-align: right;
  color: #333;
  font-size: 28rpx;
}
.form .regular{
  width: 100%;
  padding:20rpx;
}
.form .regular text{
  font-size: 24rpx;
  color: #333;
  display: block;
  padding-bottom: 10rpx;
}
.form button{
  width: 710rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: white;
  font-size: 30rpx;
  background: #568fdc;
  margin-top: 20rpx;
  margin-bottom: 50rpx;
}
.foot{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  left: 0;
  background: white;
  z-index: 100;
}
.foot>view{
  width: 33.33%;
}
.foot image{
  height:40rpx;
  display: block;
  margin:auto;
  margin-top: 20rpx;
}
.foot text{
  font-size: 20rpx;
  display: block;
  text-align: center;
  color: #999;
  padding-top: 10rpx;
}
.bg{
  position: fixed;
  z-index: 200;
  background: black;
  opacity: 0.4;
  filter: alpha(opacity=40);
  top: 0;
  width: 100%;
  height: 100%;
  left: 0;
}
.selTime{
  position: fixed;
  border: 0;
  height: 700rpx;
  background: white;
  width: 100%;
  left: 0;
  overflow-y: auto;
  bottom:0;
  z-index:300;
}
.selTime .top{
  background: #f3f3f3;
  height: 70rpx;
  line-height: 70rpx;
  width: 100%;
  position: relative;
}
.selTime .top text{
  text-align: center;
  color: #333;
  font-size: 28rpx;
  display: block;
}
.selTime .top image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.selTime .bottom>view{
  border-bottom: 1px solid #ccc;
  color: #333;
  font-size: 30rpx;
  padding:15rpx 20rpx;
}