<view class="container" style="height:{{winHeight}}px;">
  <image src="/images/infocenter/bg.jpg" class="bg"></image>
  <view class="login">
    <text>管理员登录</text>
    <form catchsubmit="formSubmit">
      <input name="username" placeholder='用户名' class="same" placeholder-style='color:white'></input>
      <input name="password" placeholder='密码' password="true" class="same" placeholder-style='color:white'></input>
      <button class="loginBtn" type='primary' formType="submit">登录</button>
    </form>
  </view>
  <navigator url="../Bosslogin/Bosslogin" class="bosslogin">超级管理员登录</navigator>
</view>
