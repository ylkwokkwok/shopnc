.container{
  background: #f6f6f6;
  position: relative;
}
.center{
  position: absolute;
  top: 0;
  left: 0;
  width:100%;
}
.center>image{
  width: 330rpx;
  display: block;
  margin:auto;
  margin-top: 200rpx;
}
.center>text,.center>navigator{
  text-align: center;
  font-size: 24rpx;
  display: block;
  margin-top: 30rpx;
}
.center>navigator{
  color: #999;
}
.center>navigator>text{
  color: red;
}