.container{
  background: #f6f6f6;
}
.tablist{
  width: 100%;
  height: 88rpx;
  background: white;
  line-height: 88rpx;
  display: flex;
  flex-direction: row;
}
.tablist>view{
  width: 25%;
  text-align: center;
  font-size: 30rpx;
}
.tablist>view.on{
  border-bottom: 5rpx solid #568fdc;
}
.tabshow .noOrder{
  width: 100%;
  margin-top: 135rpx;
}
.tabshow .noOrder image{
  width: 310rpx;
  margin:auto;
  display: block;
}
.tabshow .noOrder text{
  text-align: center;
  padding-top: 20rpx;
  color: black;
  font-size: 24rpx;
  display: block;
}
.shoplist{
  margin-top:20rpx;
  width: 750rpx;
}
.shoplist .shopone{
  width:100%;
  margin:auto;
  position: relative;
  overflow: hidden;
  background: white;
  margin-bottom: 20rpx;
  padding:0 20rpx;
}
.shoplist .shopone .sel{
  width: 40rpx;
  float: left;
  margin-top:50rpx;
}
.shoplist .shopone .left{
  float: left;
  width: 200rpx;
  position: relative;
}
.shoplist .shopone .left>image{
  width: 100%;
  margin-top:20rpx;
}
.shoplist .shopone .left>text{
  position: absolute;
  right: 0;
  top: 10rpx;
  color: red;
  font-size: 20rpx;
}
.shoplist .shopone .right{
  float: left;
  width: 490rpx;
  margin-left: 20rpx;
  overflow: hidden;
  padding-bottom: 40rpx;
}
.shoplist .shopone:last-child .right{
  border-bottom: none;
}
.shoplist .shopone .right .jie{
  width: 100%;
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  padding:0rpx 12rpx;
  margin-top:20rpx;
  color: #333;
}
.shoplist .shopone .right .price{
  font-size: 30rpx;
  color: red;
  float: left;
  padding-left: 12rpx;
}
.shoplist .shopone .right .tuan{
  width: 110rpx;
  float: left;
  position: relative;
  margin-left: 20rpx;
  margin-top:-5rpx;
}
.shoplist .shopone .right .tuan image{
  width: 100%;
}
.shoplist .shopone .right .bottom{
  width: 100%;
  padding-top: 20rpx;
}
.shoplist .shopone .right .bottom>view{
  float: left;
  font-size:30rpx;
  display:block;
  color:red;
  padding-left:20rpx;
  font-weight:bold;
}
.shoplist .shopone .right .oldprice{
  float: right;
  text-align: right;
  font-size: 20rpx;
  color: #666;
}
.shoplist .shopone .right .tishi{
  font-size:24rpx;
  padding-left:20rpx;
  color:#999;
}
.shoplist .shopone .right .oldprice text{
  font-size: 24rpx;
  text-decoration: line-through;
}
.shoplist .shopone .right .tuan view{
  position: absolute;
  width:100%;
  text-align: center;
  color: white;
  top:15rpx;
  left: 0;
  right: 0;
  font-size: 20rpx;
}
.shoplist .shopone .right .bottom navigator{
  width:120rpx;
  height:40rpx;
  float:right;
  display:block;
  background:#568fdc;
  font-size:24rpx;
  text-align:center;
  color:white;
  line-height:40rpx;
  border-radius:20rpx;
}