.container{
  background: #f6f6f6;
}
.swiper{
  position: relative;
  padding-top:80rpx;
  background: white;
  height: 520rpx;
}
.swiper>view{
  position: absolute;
  bottom:30rpx;
  right: 30rpx;
  color:#666666;
  font-size: 24rpx;
}
.swiper swiper{
  height:100%;
}
.swiper image{
  width: 400rpx;
  display: block;
  margin:auto;
}
.shops{
  background: white;
  padding:20rpx;
}
.shops>view{
  display: flex;
  flex-direction: row;
}
.shops .top .tit{
  width: 620rpx;
  font-size: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.shops .top .collect{
  width:80rpx;
  height:65rpx;
  border-left:1px solid #535353;
}
.shops .top .collect image{
  width: 32rpx;
  display: block;
  margin:auto;
}
.shops .top .collect text{
  font-size: 24rpx;
  text-align: center;
  padding-top:5rpx;
  width: 100%;
  display: block;
  color: #535353;
}
.shops .middle{
  padding-top:10rpx;
}
.shops .middle .price{
  color: red;
  font-size: 30rpx;
}
.shops .middle .tuan{
  width: 110rpx;
  float: left;
  position: relative;
  margin-left: 20rpx;
  margin-top:-5rpx;
}
.shops .middle .tuan image{
  width: 100%;
}
.shops .middle .tuan view{
  position: absolute;
  width:100%;
  text-align: center;
  color: white;
  top:15rpx;
  left: 0;
  right: 0;
  font-size: 20rpx;
}
.shops .bottom{
  padding-top: 10rpx;
}
.shops .bottom>view{
  font-size: 20rpx;
}
.shops .bottom .left{
  width: 480rpx;
  display: flex;
  flex-direction: row;
}
.shops .bottom .left .oldprice{
  color: #b3b3b3;
  width: 33.33%;
}
.shops .bottom .left .oldprice text{
  text-decoration: line-through;
}
.shops .bottom .left .salesvolumn{
  color: #e8b223;
  width:33.33%;
  text-align: center;
}
.shops .bottom .left .stock{
  width: 33.33%;
  text-align: right;
  color: #909090;
}
.shops .bottom .right{
  width: 230rpx;
}
.shops .bottom .right navigator{
  width: 125rpx;
  height: 40rpx;
  text-align: center;
  line-height: 37rpx;
  background: white;
  border:2rpx solid red;
  color: red;
  font-size: 24rpx;
  margin:auto;
  border-radius: 10rpx;
  margin-top:-10rpx;
}
.shops .returnGoods{
  margin-top:20rpx;
}
.shops .returnGoods image{
  width: 30rpx;
  margin-right: 10rpx;
}
.shops .returnGoods text{
  color: #e8b223;
  font-size: 24rpx;
}
.orderDetail{
  margin-top:20rpx;
  background: white;
  width:750rpx;
  padding:20rpx 0;
}
.orderDetail>view{
  padding:10rpx 20rpx;
  border-top:2rpx solid #ccc;
  color: #999;
  font-size: 24rpx;
}
.orderDetail>view:first-child{
  border-top:none; 
}
.orderDetail>view text{
  font-size: 30rpx;
  color: black;
  padding-left: 20rpx;
}
.shopName{
  margin-top:20rpx;
  width: 750rpx;
  background: white;
  padding:15rpx 0;
}
.shopName>text{
  text-align: center;
  font-size: 30rpx;
  display: block;
  width: 100%;
}
.shopName .middle{
  width: 100%;
  display: flex;
  flex-direction: row;
  padding-top:15rpx;
}
.shopName .middle .left{
  width: 510rpx;
  display: flex;
  flex-direction: row;
  padding-top:10rpx;
}
.shopName ul>li{
  width:33.33%;
  height: 90rpx;
  border-right: 1px solid #ccc;
}
.shopName ul>li:last-child{
  border-right: none;
}
.shopName ul>li>text{
  font-size: 24rpx;
  display: block;
  text-align: center;
}
.shopName ul>li .shuliang{
  padding-top:20rpx;
}
.shopName ul>li .name{
  padding-top:10rpx;
}
.shopName .right{
  width: 200rpx;
}
.shopName .right>view{
  text-align: center;
  color: red;
  line-height: 40rpx;
  font-size: 24rpx;
}
.shopName .bottom{
  margin:auto;
  margin-top:35rpx;
  width: 630rpx;
  overflow: hidden;
}
.shopName .bottom>navigator,.shopName .bottom>view{
  width: 255rpx;
  height: 40rpx;
  border:1px solid #ccc;
  line-height: 37rpx;
  text-align: center;
  float: left;
  font-size: 20rpx;
  color: #999;
}
.shopName .bottom .joinstore{
  float: right;
}
.collaging,.collage_list,.evaluate{
  margin-top:20rpx;
  background: white;
  width: 750rpx;
}
.collaging .top,.evaluate .top{
  height: 88rpx;
  line-height: 88rpx;
  width: 710rpx;
  margin:auto;
}
.evaluate .top{
  font-size: 30rpx;
}
.evaluate .top text{
  color: red;
  font-size: 26rpx;
  padding-left: 10rpx;
}
.evaluate .evaluate_one{
  border-bottom:1px solid  #ccc;
  width:710rpx;
  margin:auto;
  padding:20rpx 0;
}
.evaluate .evaluate_one .up{
  overflow: hidden;
  line-height: 25rpx;
}
.evaluate .evaluate_one .up .username,.evaluate .evaluate_one .up .date{
  color: #666666;
  font-size: 26rpx;
  float: left;
}
.evaluate .evaluate_one .up .star{
  float: right;
}
.evaluate .evaluate_one .up .star image{
  width:30rpx;
  margin-right: 5rpx;
}
.evaluate .evaluate_one .up .date{
  padding-left: 20rpx;
}
.evaluate .evaluate_one .bottom{
  padding-top:20rpx;
}
.evaluate .evaluate_one .bottom .content{
  font-size: 28rpx;
}
.evaluate .evaluate_one .bottom .shopsimg{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}
.evaluate .evaluate_one .bottom .shopsimg image{
  width: 120rpx;
  height: 120rpx;
  border:1px solid #ccc;
  margin-right: 20rpx;
}
.evaluate>navigator{
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  font-size: 26rpx;
  letter-spacing: 3rpx;
}
.swiper-tab{
  width: 750rpx;
  padding:20rpx;
  background: white;
  display: flex;
  flex-direction: row;
  margin-top:20rpx;
}
.swiper-tab .swiper-tab-list{
  margin-right: 50rpx;
  font-size: 30rpx;
}
.swiper-tab .on{
  color: #568fdc;  
  border-bottom: 5rpx solid #568fdc;
}
.swiper-box{
  background: white;
  padding:20rpx;
}
.foot{
  position: fixed;
  bottom:0;
  width: 750rpx;
  height: 90rpx;
  background: white;
  display: flex;
  flex-direction: row;
}
.foot .nosame{
  width: 100%;
  height: 100%;
  line-height:90rpx;
  color: white;
  background: #568fdc;
  font-size: 30rpx;
  text-align: center;
}
.foot .left{
  width: 45%;
  height: 100%;
  display: flex;
  flex-direction: row;
}
.foot .left>navigator{
  width: 33.33%;
  height: 100%;
}
.foot .right{
  width: 55%;
  height: 100%;
  display: flex;
  flex-direction: row;
}
.foot .left>navigator image{
  width:36rpx; 
  height: 34rpx; 
  display: block;
  margin:auto;
  margin-top:40rpx;
}
.foot .left>navigator view{
  font-size: 20rpx;
  color: #676767;
  text-align: center;
  padding-top: 5rpx;
  letter-spacing: 2rpx;
}
.foot .right>navigator{
  line-height: 120rpx;
  width:50%;
  color: white;
}
.foot .right .second{
  background: #568fdc;
}
.foot .right .thrid{
  background: #e8b223;
}
.foot .right>navigator .money{
  font-size: 36rpx;
  text-align: center;
  font-family: "宋体";
}
.foot .right>navigator .money>text{
  padding-left: 10rpx;
  font-size: 20rpx;
  font-family: "微软雅黑";
}
.foot .second .type{
  font-size: 20rpx;
  padding-left: 10rpx;
}