page{
  background-color: #f6f6f6;
}

.address-info{
  padding: 10px;
  background-color: #ffffff;
  margin-bottom: 10px;
  display: flex;
  align-items:center;
  flex-direction: row;
}

.address-info .location-icon{
  width: 16px;
  height: 16px;
  vertical-align: middle;
  margin-right: 5px;
}

.address-info .location-info{
  display: flex;
  flex: 1;
  align-items:center;
  justify-content:space-between;
  font-size: 16px;
  vertical-align: middle;
}

.address-info .location-info .address-name{
  font-weight: bold;
  padding-right: 20px;
}

.address-info .location-info .address-tel{
  font-weight: bold;
}

.address-info .location-info .address-detail{
  font-size: 14px;
}

.address-info .location-info::after {
  content:'';
  display:inline-block;
  height:10rpx;
  width:10rpx;
  border:0 solid #ccc;
  border-width:3rpx 3rpx 0 0;
  transform: matrix(1, 1, -1, 1, 0, 0);
}
.address-btn{
  font-size: 30rpx;
  color: #ffffff;
  background-color: #999999;
  border-radius: 10rpx;
}
/*  */
.store-cart{
  border-top: 1px solid #eeeeee;
  border-bottom: 1px solid #eeeeee;
  margin-bottom: 20px;
  background: #ffffff;
}

.store-info{
  line-height: 45px;
  border-bottom: 1px solid #eeeeee;
  padding: 0 10px;
}

.store-info radio{
  vertical-align: middle;
  margin-right: 5px;
}
.store-info .store-icon{
  width: 16px;
  height: 16px;
  vertical-align: middle;
  margin-right: 5px;
}
.store-info .store-name{
  display: inline-block;
  font-size: 16px;
  color: #666666;
  vertical-align: middle;
}

.goods-list{
  padding-left: 10px;
  padding-right: 10px;
}

.goods-list .goods{
  padding-top: 15px;
  padding-bottom: 15px;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #eeeeee;
}
.goods-list .goods:last-child{
  border-bottom: none;
}

.goods-list .goods radio{
  display: flex;
  vertical-align: middle;
  margin-right: 5px;
}

.goods-list .goods .goods-image-wrapper{
  display: flex;
  margin-right: 5px;
}

.goods-list .goods .goods-image{
  display: inline-block;
  padding: 5px;
  border: 1px solid #eeeeee;
  border-radius: 5px;
  vertical-align: middle;
}

.goods-list .goods .goods-image image{
  width: 70px;
  height: 70px;
  border-radius: 5px;
  vertical-align: middle;
}

.goods-list .goods .goods-infos-wrapper{
  flex: 1;
  flex-grow: 1;
  overflow: hidden;
}

.goods-list .goods .goods-infos-wrapper .goods-name-wrapper{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.goods-list .goods .goods-infos-wrapper .goods-name-wrapper .goods-name{
  display: flex;
  flex-grow: 1;
  flex: 1;
  height: 30px;
  font-size: 28rpx;
  color: #333333;
  padding-right: 5px;
  line-height: 15px;
  overflow: hidden;
}

.goods-list .goods .goods-infos-wrapper .goods-name-wrapper .goods-del{
  width: 16px;
  height: 16px;
}

.goods-list .goods .goods-infos-wrapper .goods-jingle{
  font-size: 12px;
  line-height: 20px;
  color: #666666;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.goods-list .goods .goods-infos-wrapper .goods-price{
  display: inline-block;
  font-size: 16px;
  font-weight: bold;
  padding-top: 9px;
  color: #DB4453;
}

.goods-list .goods .goods-infos-wrapper .inc-num-dec-group{
  display: inline-block;
  float: right;
}

.goods-list .goods .goods-infos-wrapper .goods-amount{
  float: right;
  display: inline-block;
  padding-right: 10px;
  padding-top: 12px;
  color: #333;
  font-size: 28rpx;
}
.buy-remarks{
  margin-top: 20rpx;
  height: 200rpx;
  background-color: #ffffff;
  padding: 10rpx 20rpx;
}
.buy-remarks textarea{
  width: 690rpx;
  height: 160rpx;
  border: 1px solid #bfbfbf;
  background-color: #eeeeee;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999999;
  border-radius: 20rpx;
  padding: 10rpx;
}
.buy-price{
  margin-top: 20rpx;
  background-color: #ffffff;
}
.buy-price view{
  height: 70rpx;
  line-height: 70rpx;
  margin: 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.buy-price view text:nth-child(2n+1) {
  font-size: 24rpx;
  color: #262626;
}
.buy-price view text:nth-child(2n+2){
  font-size: 24rpx;
  color: #ff0000;
}
.buy-btns button{
  margin: 20rpx 20rpx 0 20rpx;
  width: 710rpx;
  height: 90rpx;
  font-size: 30rpx;
  color: #ffffff;
  border-radius: 10rpx;
}

.address{
  width: 750rpx;
  background: white;
  height: 100rpx;
  padding-left: 20rpx;
}
.address>image{
  width: 21rpx;
  margin-top: 39.5rpx;
  margin-right: 20rpx;
}
.address text{
  font-size: 30rpx;
  color: #999;
}
/* address */
.overflow-hidden{
  overflow: hidden;
}
.overflow-auto{
  overflow: auto;
}
.bg{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 200;
  background-color: rgba(0,0,0,.3);
}
.alert-box{
  position: fixed;
  width: 100%;
  height: 60%;
  left: 0;
  bottom: 0;
  z-index: 300;
  background-color: #ffffff;
  overflow: auto;
  font-size: 30rpx;
  color: #333333;
}
.alter-header{
  height: 70rpx;
  display: flex;
  justify-content: center;
  background-color: #eeeeee;
  position: relative;
}
.alter-header text{
  display: flex;
  font-size: 36rpx;
  line-height: 70rpx;
}
.alter-header image{
  width: 36rpx;
  height: 36rpx;
  position:absolute;
  right: 20rpx;
  top: 22rpx;
  vertical-align: middle;
}
.address-list{
  display: flex;
  flex-direction: column;
}
.address-item{
  display: flex;
  flex-direction: row;
  height: 128rpx;
  border-bottom: 1px solid #eeeeee;
}
.address-item radio{
  margin-left: 20rpx;
  margin-top: 50rpx;
}
.address-item-info{
  width: 100%;
  margin: 32rpx 20rpx;
  display: flex;
  flex-direction: column;
  line-height: 30rpx;
}
.address-item-icon{
  display: flex;
  justify-content: center;
  margin-right: 20rpx;
}
.address-item-icon image{
  display: flex;
  width: 36rpx;
  height: 36rpx;
  margin-top: 46rpx;
  vertical-align: middle;
}
.add-address image{
  margin-left: 20rpx;
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}
.add-address view{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 30rpx;
}
.no-data>view,.no-data>navigator{
  text-align: center;
  display: block;
  margin-top: 20rpx;
}
.no-data>view{
  font-size: 28rpx;
  color: #333;
}
.no-data>navigator{
  font-size: 24rpx;
  color: #568fdc;
}



