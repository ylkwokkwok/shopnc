.container{
  background: #f6f6f6;
}
.swiper{
  position: relative;
  padding-top:80rpx;
  background: white;
  height: 520rpx;
}
.swiper>view{
  position: absolute;
  bottom:30rpx;
  right: 30rpx;
  color:#666666;
  font-size: 24rpx;
}
.swiper swiper{
  height:100%;
}
.swiper image{
  width: 400rpx;
  display: block;
  margin:auto;
}
.shops{
  background: white;
  padding:20rpx;
}
.shops>view{
  display: flex;
  flex-direction: row;
}
.shops .top .tit{
  width: 630rpx;
  font-size: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.shops .top .collect{
  width:80rpx;
  height:65rpx;
  border-left:1px solid #535353;
}
.shops .top .collect image{
  width: 32rpx;
  display: block;
  margin:auto;
}
.shops .top .collect text{
  font-size: 24rpx;
  text-align: center;
  padding-top:5rpx;
  width: 100%;
  display: block;
  color: #535353;
}
.shops .middle{
  padding-top:10rpx;
}
.shops .middle .price{
  color: red;
  font-size: 30rpx;
}
.shops .middle .tuan{
  width: 110rpx;
  float: left;
  position: relative;
  margin-left: 20rpx;
  margin-top:-5rpx;
}
.shops .middle .tuan image{
  width: 100%;
}
.shops .middle .tuan view{
  position: absolute;
  width:100%;
  text-align: center;
  color: white;
  top:15rpx;
  left: 0;
  right: 0;
  font-size: 20rpx;
}
.shops .bottom{
  padding-top: 10rpx;
}
.shops .bottom>view{
  font-size: 20rpx;
}
.shops .bottom .oldprice{
  color: #b3b3b3;
  width:50%;
}
.shops .bottom .oldprice text{
  text-decoration: line-through;
}
.shops .bottom .right{
  width:50%;
  display: flex;
  flex-direction: row;  
}
.shops .bottom .right .salesvolumn{
  color: #ffb221;
  width: 50%;
}
.shops .bottom .right .stock{
  width: 50%;
  text-align: right;
  color: #909090;
}
.introduce{
  height: 88rpx;
  margin-top:20rpx;
  width:750rpx;
  line-height: 88rpx;
  background: white;
}
.introduce .left{
  float: left;
  padding-left: 20rpx;
  color: #666666;
}
.introduce image{
  float: right;
  margin-right: 20rpx;
  width: 20rpx;
  margin-top: 32rpx;
}
.collaging,.collage_list,.evaluate{
  margin-top:20rpx;
  background: white;
  width: 750rpx;
}
.collaging .top,.evaluate .top{
  height: 88rpx;
  line-height: 88rpx;
  width: 710rpx;
  margin:auto;
}
.evaluate .top{
  font-size: 30rpx;
}
.collaging .top>view{
  font-size: 30rpx;
  float: left;
}
.collaging .top>navigator{
  float: right;
}
.collaging .top>navigator>text{
  font-size: 20rpx;
  color: #b9b9b9;
  float: left;
}
.collaging .top>navigator>image{
  width: 15rpx;
  float: left;
  margin-top:32rpx;
  margin-left: 10rpx;
}
.collage_one{
  width: 710rpx;
  margin:auto;
  display: flex;
  flex-direction: row;
  height: 75rpx;
  line-height: 75rpx;
  padding-bottom:20rpx;
}
.collage_one image{
  width: 75rpx;
  height: 75rpx;
  border-radius: 50%;
}
.collage_one .username{
  font-size: 30rpx;
  padding-left: 20rpx;
}
.collage_one .gray_bg{
  width: 280rpx;
  height: 35rpx;
  background: #f4f4f4;
  line-height: 35rpx;
  margin-top:20rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
  margin-left: 90rpx;
}
.collage_one .gray_bg .first{
  font-size: 20rpx;
  color: #acacac;
  width: 50%;
  text-align: center;
}
.collage_one .gray_bg .time{
  font-size: 24rpx;
  color: #919191;
  width: 50%;
  text-align: center;
}
.collage_one navigator{
  border:1px solid red;
  border-radius: 10rpx;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 17.5rpx;
  font-size: 24rpx;
  text-align: center;
  color:red;
  margin-left: 30rpx;
}
.evaluate .evaluate_one{
  border-bottom:1px solid  #ccc;
  width:710rpx;
  margin:auto;
  padding:20rpx 0;
}
.evaluate .evaluate_one .up{
  overflow: hidden;
  line-height: 25rpx;
}
.evaluate .evaluate_one .up .username,.evaluate .evaluate_one .up .date{
  color: #666666;
  font-size: 26rpx;
  float: left;
}
.evaluate .evaluate_one .up .star{
  float: right;
}
.evaluate .evaluate_one .up .star image{
  width:30rpx;
  margin-right: 5rpx;
}
.evaluate .evaluate_one .up .date{
  padding-left: 20rpx;
}
.evaluate .evaluate_one .bottom{
  padding-top:20rpx;
}
.evaluate .evaluate_one .bottom .content{
  font-size: 28rpx;
}
.evaluate .evaluate_one .bottom .shopsimg{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}
.evaluate .evaluate_one .bottom .shopsimg image{
  width: 120rpx;
  height: 120rpx;
  border:1px solid #ccc;
  margin-right: 20rpx;
}
.evaluate>navigator{
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  font-size: 26rpx;
  letter-spacing: 3rpx;
}
.swiper-tab{
  width: 750rpx;
  padding:20rpx;
  background: white;
  display: flex;
  flex-direction: row;
  margin-top:20rpx;
}
.swiper-tab .swiper-tab-list{
  margin-right: 50rpx;
  font-size: 30rpx;
}
.swiper-tab .on{
  color: #568fdc;  
  border-bottom: 5rpx solid #568fdc;
}
.swiper-box{
  background: white;
  padding:20rpx;
}
.foot{
  position: fixed;
  bottom:0;
  width: 750rpx;
  height: 80rpx;
  background: white;
  display: flex;
  flex-direction: row;
}
.foot>navigator{
  width: 33.33%;
  height: 100%;
}
.foot .first image{
  width:32rpx;
  display: block;
  margin:auto;
  margin-top:10rpx;
}
.foot .first view{
  font-size: 20rpx;
  color: #676767;
  text-align: center;
  padding-top: 5rpx;
  letter-spacing: 2rpx;
}
.foot .second{
  background: #ffb221;
  color: white;
  line-height: 80rpx;
}
.foot>navigator .money{
  font-size: 36rpx;
  text-align: center;
  font-family: "宋体";
}
.foot>navigator .money>text{
  padding-left: 10rpx;
  font-size: 20rpx;
  font-family: "微软雅黑";
}
.foot .second .type{
  font-size: 20rpx;
  padding-left: 10rpx;
}
.foot .thrid{
  background: #568fdc;
  color: white;
  line-height: 80rpx;
}