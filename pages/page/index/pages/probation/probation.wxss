.container{
  background: #f6f6f6;
}
.swiper{
  position: relative;
  padding-top:80rpx;
  background: white;
  height: 520rpx;
}
.swiper>view{
  position: absolute;
  bottom:30rpx;
  right: 30rpx;
  color:#666666;
  font-size: 24rpx;
}
.swiper swiper{
  height:100%;
}
.swiper image{
  width: 400rpx;
  display: block;
  margin:auto;
}
.shops{
  background: white;
  padding:20rpx;
}
.shops>view{
  display: flex;
  flex-direction: row;
}
.shops .top .tit{
  width: 620rpx;
  font-size: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.shops .top .collect{
  width:80rpx;
  height:65rpx;
  border-left:1px solid #535353;
}
.shops .top .collect image{
  width: 32rpx;
  display: block;
  margin:auto;
}
.shops .top .collect text{
  font-size: 24rpx;
  text-align: center;
  padding-top:5rpx;
  width: 100%;
  display: block;
  color: #535353;
}
.shops .middle{
  padding-top:10rpx;
  display:block;
  overflow:hidden;
}
.shops .middle .price{
  color: red;
  font-size: 30rpx;
  float: left;
}
.shops .middle .count{
  float: right;
  font-size: 24rpx;
  color: #999;
}
.shops .middle .count text{
  color: red;
}
.shops .middle .tuan{
  width: 110rpx;
  float: left;
  position: relative;
  margin-left: 20rpx;
  margin-top:-5rpx;
}
.shops .middle .tuan image{
  width: 100%;
}
.shops .middle .tuan view{
  position: absolute;
  width:100%;
  text-align: center;
  color: white;
  top:15rpx;
  left: 0;
  right: 0;
  font-size: 20rpx;
}
.shops .bottom{
  padding-top: 10rpx;
}
.shops .bottom>view{
  font-size: 20rpx;
}
.shops .bottom .oldprice{
  color: #b3b3b3;
}
.shops .bottom .oldprice text{
  text-decoration: line-through;
}
.shops .bottom .people{
  margin-left: 50rpx;
  font-size: 20rpx;
  color: #808080;
}
.xq{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background: white;
  margin-top: 20rpx;
  overflow: hidden;
}
.xq navigator{
  width: 100%;
  overflow: hidden;
  display: block;
}
.xq navigator text{
  float: left;
  padding-left: 20rpx;
  font-size: 30rpx;
}
.xq navigator image{
  width: 15rpx;
  margin-top: 30rpx;
  float: right;
  margin-right: 20rpx;
}
.collaging,.collage_list,.evaluate{
  margin-top:20rpx;
  background: white;
  width: 750rpx;
}
.collaging .top,.evaluate .top{
  height: 88rpx;
  line-height: 88rpx;
  width: 710rpx;
  margin:auto;
}
.evaluate .top{
  font-size: 30rpx;
}
.evaluate .top text{
  color: red;
  font-size: 26rpx;
  padding-left: 10rpx;
}
.evaluate .evaluate_one{
  border-bottom:1px solid  #ccc;
  width:710rpx;
  margin:auto;
  padding:20rpx 0;
}
.evaluate .evaluate_one .up{
  overflow: hidden;
  line-height: 25rpx;
}
.evaluate .evaluate_one .up .username,.evaluate .evaluate_one .up .date{
  color: #666666;
  font-size: 26rpx;
  float: left;
}
.evaluate .evaluate_one .up .star{
  float: right;
}
.evaluate .evaluate_one .up .star image{
  width:30rpx;
  margin-right: 5rpx;
}
.evaluate .evaluate_one .up .date{
  padding-left: 20rpx;
}
.evaluate .evaluate_one .bottom{
  padding-top:20rpx;
}
.evaluate .evaluate_one .bottom .content{
  font-size: 28rpx;
}
.evaluate .evaluate_one .bottom .shopsimg{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}
.evaluate .evaluate_one .bottom .shopsimg image{
  width: 120rpx;
  height: 120rpx;
  border:1px solid #ccc;
  margin-right: 20rpx;
}
.evaluate>navigator{
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  font-size: 26rpx;
  letter-spacing: 3rpx;
}
.swiper-tab{
  width: 750rpx;
  padding:20rpx;
  background: white;
  display: flex;
  flex-direction: row;
  margin-top:20rpx;
}
.swiper-tab .swiper-tab-list{
  margin-right: 50rpx;
  font-size: 30rpx;
}
.swiper-tab .on{
  color: #568fdc;  
  border-bottom: 5rpx solid #568fdc;
}
.swiper-box{
  background: white;
  padding:20rpx;
}
.foot{
  position: fixed;
  bottom:0;
  width: 750rpx;
  height: 90rpx;
  background: white;
  display: flex;
  flex-direction: row;
}
.foot .probation{
  width: 190rpx;
}
.foot .probation image{
  width: 33rpx;
  display: block;
  margin:auto;
  margin-top: 20rpx;
}
.foot .probation text{
  text-align: center;
  display: block;
  color: #8e8e8e;
  font-size: 20rpx;
}
.foot .applay{
  width: 560rpx;
  background: #568fdc;
  font-size: 30rpx;
}
.foot .applay navigator{
  width: 100%;
  height: 100%;
  text-align: center;
  color: white;
  line-height: 90rpx;
}